# cPanel MySQL Database Configuration
# Replace these values with your actual cPanel database credentials

# Database Host (usually your domain name or server IP)
DB_HOST=localhost

# Database Name (local database)
DB_NAME=snag_management

# Database Username (local MySQL root)
DB_USER=root

# Database Password (empty for local)
DB_PASSWORD=

# Database Port (usually 3306 for MySQL)
DB_PORT=3306

# Flask Configuration
FLASK_SECRET_KEY=your-secret-key-here
FLASK_ENV=development
